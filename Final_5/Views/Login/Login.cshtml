@model Final_5.Models.Users

<body style="height: 100%; width: 100%;">

    <div id="middle" style="height: 600px; width: 100%; margin-top: 25px;">

        <div style="width:10%;  margin-left:-6%; float: left ">
            <h1 style="font-family:'Bookman Old Style';color:orangered; margin-left:30px;margin-top:-5px">MediCare</h1>
            <div class="panel-group" style="width:240px;">

                <div class="panel panel-primary">
                    <div class="panel-heading" style="width:240px"><h3>Search Doctor</h3></div>
                    <div class="panel-body" style="margin-top:-12%">
                        @using (Html.BeginForm())
                        { 
                        @*<form id="SearchDoc">*@
                            @Html.AntiForgeryToken()
                           
                            <div class="form-horizontal">
                                <hr />
                                @Html.ValidationSummary(true, "", new { @class = "text-warning" })

                                <div class="form-group">
                                    <h4 style="margin-left:20px">Choose a Practic:</h4>
                                    <div class="col-md-12">
                                        @Html.DropDownList("practicsName", null, htmlAttributes: new { @class = "form-control" })
                                    </div>
                                </div>                                
                                @*<br/>*@

                                <div class="form-group">
                                    <h4 style="margin-left:20px">Choose a City:</h4>
                                    <div class="col-md-12">
                                        @Html.DropDownList("city", null, htmlAttributes: new { @class = "form-control" })
                                    </div>
                                </div>
                                <br />

                                <div class="form-group">
                                    <div class="col-md-10">
                                        @*<a href="#" class="btn btn-primary" onclick="SearchDoc()" style="height:40px; width:100px; margin-left:0px"><p style="font-size:130%">Search</p></a>*@
                                        <button type="submit" class="btn btn-success btn-lg" style="margin-left:50px"> <span class="glyphicon glyphicon-search"></span> Search</button>
                                    </div>
                                </div>

                            </div>
                            }
                        @*</form>*@
                    </div>
                </div>
            </div>
        </div>

        <div class="panel panel-default" style="width:65%;  margin-left:13%; float: left">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                @*<div class="col-md-10">*@
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                    <li data-target="#myCarousel" data-slide-to="3"></li>
                    @*<li data-target="#myCarousel" data-slide-to="4"></li>*@
                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner">
                    <div class="item active">
                        <img src="~/Content/Images/1.jpg" class="img-rounded" style="width:100% ; height:100%" />
                    </div>

                    <div class="item">
                        <img src="~/Content/Images/2.jpg" class="img-rounded" style="width:100% ; height:100%" />
                    </div>

                    <div class="item">
                        <img src="~/Content/Images/4.jpg" class="img-rounded" style="width:100% ; height:100%" />
                    </div>

                    <div class="item">
                        <img src="~/Content/Images/5.jpg" class="img-rounded" style="width:100% ; height:100%" />
                    </div>

                    @*<div class="item">
                        <img src="~/Content/Images/3.jpg" class="img-rounded" style="width:100% ; height:100%" />
                    </div>*@
                </div>

                <!-- Left and right controls -->
                <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                    @*<span class="glyphicon glyphicon-chevron-left"></span>*@
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" data-slide="next">
                    @*<span class="glyphicon glyphicon-chevron-right"></span>*@
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
        @*</div>*@

        <div style="width:15%; float:left; height:750px; ">
            <div class="panel-group" style="width:240px; margin-left:15% ">
                <div class="panel panel-primary">
                    <div class="panel-heading" style="width:240px"><h3>Login</h3></div>
                    <div class="panel-body" style="margin-top:-12%">
                        @*@using (Html.BeginForm())
                            {*@
                        <form id="myForm">
                            @Html.AntiForgeryToken()

                            <div class="form-horizontal">
                                @*<h4>User</h4>*@
                                <hr />
                                @Html.ValidationSummary(true, "", new { @class = "text-warning" })
                                <div class="form-group">
                                    @*@Html.LabelFor(model => model.userId, "UserID:", htmlAttributes: new { @class = "control-label col-md-4" })
                                        <br /><br />*@
                                    <div class="col-md-12">
                                        @Html.TextBoxFor(model => model.userId, new { @class = "form-control", @placeholder = "ID Number" })
                                        @*@Html.ValidationMessageFor(model => model.userId, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>

                                <div class="form-group">
                                    @*@Html.LabelFor(model => model.password, "Password:", htmlAttributes: new { @class = "control-label col-md-4" })
                                        <br /><br />*@
                                    <div class="col-md-12">
                                        @Html.PasswordFor(model => model.password, new { @class = "form-control", @placeholder = "Password" })
                                        @*@Html.ValidationMessageFor(model => model.password, "", new { @class = "text-danger" })*@
                                    </div>
                                </div>
                                <br />
                                <div class="form-group">
                                    <div class="col-md-10">
                                        @*<input type="submit" value="Login" class="btn btn-primary" style="height:40px; width:100px; margin-left:55px" />*@
                                        @*<a href="#" class="btn btn-primary" onclick="LoginUser()" style="height:40px; width:100px; margin-left:55px"><p style="font-size:130%">Login</p></a>*@
                                        <a href="#" class="btn btn-primary btn-lg" onclick="LoginUser()" style="width:110px; margin-left:50px">Login</a>
                                    </div>
                                </div>
                            </div>
                            @*}*@
                        </form>
                        @*<div>
                                @Html.ActionLink("Back to List", "Index")
                            </div>*@
                    </div>
                </div>
            </div>
            <br />

            @*<div class="panel-group" style="width:270px; margin-left:25%;">
                    <div class="panel panel-primary">
                        <div class="panel-heading" style="width:270px"><h3>Search Doctor</h3></div>
                        <div class="panel-body">

                        </div>
                    </div>
                </div>*@

            <div style="margin-left:55%">
                <form id="myForm2">
                    @Html.AntiForgeryToken()
                    <div class="form-horizontal">
                        <div>
                            <button type="button" onclick="LoginFinger()" style="background-color:lightgoldenrodyellow; border-bottom-color:none;border:none ">
                                <img src="~/Content/Images/invalid3.png" />
                            </button>
                        </div>
                    </div>
                </form>
            </div>

        </div>
    </div>
</body>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<script>
    var LoginFinger = function () {
        // debugger
        var data = $("#myForm2").serialize();

        $.ajax({
            type: "Post",
            url: "/Login/loginFinger",
            data: data,
            success: function (response) {
                // debugger
                if (response == "fail") {
                    alert("Login INVALID! Please try again.")
                    window.location.href = "/Login/Login";
                }
                else if (response != "fail") {
                    window.location.href = "/AnotherArea/Home2/afterLog";
                }
            }

        })
    };

    var LoginUser = function () {
        // debugger
        var data = $("#myForm").serialize();

        $.ajax({
            type: "Post",
            url: "/Login/Login1",
            data: data,
            success: function (response) {
                // debugger
                if (response == "fail") {
                    alert("Login or Password INVALID! Please try again.")
                    window.location.href = "/Login/Login";
                }
                if (response == "reg_user") {
                    window.location.href = "/Home/Index";
                }
                if (response == "fin_user") {
                    window.location.href = "/AnotherArea/Home2/afterLog";
                }
            }

        })
    };

    window.onload = function speak() {
        var msg = new SpeechSynthesisUtterance();
        //msg.text = "Wellcome, this web site allowing logging in to an account via a fingerprint, in order to do so please press Q on the key board";
        msg.text = "Wellcome, please press Q on the key board to login";
        window.speechSynthesis.speak(msg);
    };

    document.onkeydown = function (e) {
        if (e.key === "q") {
            var msg = new SpeechSynthesisUtterance();
            msg.text = "after hearing a beep sound you would be logged";
            window.speechSynthesis.speak(msg);
            LoginFinger();
            if (LoginFinger != "")
                beep();
        }
    };

    function beep() {
        var snd = new Audio("data:audio/wav;base64,");
        snd.play();
    };



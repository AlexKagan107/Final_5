@model Final_5.Models.DateTimeByDoctor

@{
    ViewBag.Title = "Delete";
}

<h2>Delete the turn</h2>

<h3>Are you sure you want to delete this?</h3>
<div style="margin-left:30%; font-size:17px">
    @*<h4>DateTimeByDoctor</h4>*@
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Doctor Practic:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.practicsName)
        </dd>

        <dt>
            Doctor ID:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.idDoctor)
        </dd>

        <dt>
            Doctor Name:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Doctor.firstName)
        </dd>

        <dt>
            Date Of Turn:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.insertDate)
        </dd>

        <dt>
            City:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.city)
        </dd>

        <dt>
            Turn ID:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.turnId)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color" style="margin-left:65px; margin-top:25px">
            <input type="submit" value="Delete" class="btn btn-danger btn-lg" /> |
            <a class="btn btn-info  btn-lg" href="~/DateTimeByDoctor/Index">Back to List</a>
        </div>
    }
</div>
